{
  "name": "hockey-stats",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.3.0",
    "flexboxgrid": "^6.3.1",
    "history": "^4.7.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-big-calendar": "^0.20.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "reactstrap": "^6.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1"
  },
  "scripts": {
    "build-css": "node-sass src/styles/sass/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass src/styles/sass/ -o src/styles/ --watch --recursive --usePolling",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
